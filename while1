package Aula_13_Lista;

import java.util.Scanner;

public class rptwhile1 {

	public static void main(String[] args) {
		 Scanner ler = new Scanner(System.in);
	        int numero;

	        System.out.print("Digite um valor limite: ");
	        numero = ler.nextInt();
	        System.out.print("Números ímpares no intervalo de " + numero + ": ");
	        int i = 1;
	        while (i <= numero) {
	           if (i % 2 != 0) {
	                System.out.print(i + " ");
	            }
	            i++; // Incremento
	        }
	}

}
